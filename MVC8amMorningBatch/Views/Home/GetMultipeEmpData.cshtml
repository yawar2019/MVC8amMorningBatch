 
@model List<MVC8amMorningBatch.Models.Employee>
@{
    ViewBag.Title = "GetMultipeEmpData";
}

<h2>GetMultipeEmpData</h2>

<table class="table-">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>